    excludes_analyse:
        - 'vendor/*'

    ignoreErrors:
        # Symfony 3.4 compatibility
        - '/Call to function method_exists\(\) with .Symfony\\\\Componentâ€¦. and .getRootNode. will always evaluate to false/'
